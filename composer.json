{
    "name": "cometcast/openapi-php-sdk-laravel",
    "description": "",
    "keywords": [
        "cometcast",
        "php-sdk-laravel"
    ],
    "homepage": "https://github.com/cometcast/php-sdk-laravel",
    "license": "Apache-2.0",
    "type": "library",
    "authors": [
        {
            "name": "Chou, Yu-Hsien",
            "email": "slps970093@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "cometcast/openapi-php-sdk": "v20251118-beta",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Cometcast\\OpenApi\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cometcast\\OpenApi\\Laravel\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cometcast\\OpenApi\\Laravel\\PhpSdkLaravelServiceProvider"
            ]
        }
    }
}
